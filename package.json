{
  "name": "Real-time-CRUD",
  "version": "0.0.3",
  "description": "Easy to configure, Real-time CRUD Database Web Application",
  "author": "Sebastian Schmidt",
  "repository": "https://github.com/publicarray/Real-time-CRUD",
  "license": "Apache-2.0",
  "dependencies": {
    "@bower_components/bootstrap": "twbs/bootstrap#^4.1.1",
    "@bower_components/jquery": "jquery/jquery#^3.3.1",
    "@bower_components/tablesort": "tristen/tablesort#^4.1.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "bootstrap": "4.2.1",
    "cookie-session": "^2.0.0-beta.3",
    "dot-emc": "^0.1.2",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "jquery": "jquery/jquery#^3.3.1",
    "knex": "^0.16.0",
    "mysql": "^2.15.0",
    "pg": "^7.4.3",
    "sanitizer": "^0.1.3",
    "socket.io": "^2.1.1",
    "sqlite3": "^4.0.1",
    "tablesort": "tristen/tablesort#^4.1.0"
  },
  "scripts": {
    "first-run": "yarn install --production",
    "first-run-dev": "yarn",
    "update-all": "yarn upgrade --latest; npm shrinkwrap",
    "build": "gulp",
    "start": "node server",
    "test": "snyk test; xo"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "snyk": "^1.88.2",
    "xo": "^0.24.0"
  },
  "xo": {
    "space": true,
    "ignores": [
      "public/lib/**"
    ],
    "rules": {
      "no-var": 0,
      "prefer-const": 0,
      "prefer-spread": 0,
      "prefer-reflect": 0,
      "prefer-template": 0,
      "require-yield": 0,
      "arrow-parens": 0,
      "object-shorthand": 1,
      "prefer-arrow-callback": 0
    }
  }
}
