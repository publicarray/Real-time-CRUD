  <a href="events/TW">
    <div class="col-xs-6 col-md-3">
      <div class="panel status panel-danger">
        <div class="panel-heading">
          <h1 class="panel-title text-center">TW</h1>
        </div>
      </div>
    </div>
  </a>
  <a href="events/TR">
    <div class="col-xs-6 col-md-3">
      <div class="panel status panel-warning">
        <div class="panel-heading">
          <h1 class="panel-title text-center">TR</h1>
        </div>
      </div>
    </div>
  </a>
  <a href="events/EF">
    <div class="col-xs-6 col-md-3">
