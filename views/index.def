{{#def.include('layout')}}
{{##def.style: #rings{text-align: center;} #rings a{display: inline-block; margin: 0;}@media(min-width: 500px){#rings a{margin: 2%;}#}}
{{##def.head:<link href="/lib/tablesort.css" rel="stylesheet">#}}
{{##def.content:
<!-- <div class="col-sm-12" id="rings">
<h2>Rings</h2>
  <a href="/1" class="btn btn-lg btn-primary">1</a>
  <a href="/2" class="btn btn-lg btn-primary">2</a>
  <a href="/3" class="btn btn-lg btn-primary">3</a>
  <a href="/4" class="btn btn-lg btn-primary">4</a>
  <a href="/5" class="btn btn-lg btn-primary">5</a>
</div> -->
<table class="table table-hover table-striped" id="table">
  <thead>
    <tr>
      {{ for(var col in it.events[0]) { }}
        {{? col !== 'id' && col !== 'orderNo'}}<th>{{!col}}</th>{{?}}
      {{ } }}
<!--       <th>Event</th>
      <th>Ring</th>
      <th># of Competitors</th>
      <th>Event Finished</th> -->
    </tr>
  </thead>
  <tbody id="data">
  {{~it.events :row}}
    <tr id="{{!row.id}}" {{? row.done === 1 }}class="success"{{?}}>
    {{ for(var item in row) { }}
      {{? item === 'done' && row.done === 0 }}
      <td>No</td>
      {{?? item === 'done' && row.done === 1 }}
      <td>Yes</td>
      {{?? item !== 'id' && item !== 'orderNo'}}
      <td>{{!row[item]}}</td>
      {{?}}
    {{ } }}
    </tr>
  {{~}}
  </tbody>
</table>
#}}
{{##def.script:
<script src='/lib/tablesort.min.js'></script>
<script src="/js/client.js"></script>
#}}
